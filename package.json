{
  "name": "orcafs-gui",
  "version": "1.0.0",
  "description": "gui for orcafs, developed by the web development team of orcadt.",
  "contributors": [
    "Wei Cao <wei.cao@orcadt.com>",
    "Xiaoyu Liu <xiaoyu.liu@orcadt.com>",
    "Yang Xiang <yang.xiang@orcadt.com>",
    "Pengcheng Li <pengcheng.li@orcadt.com>"
  ],
  "author": "Orcadt",
  "private": true,
  "repository": {
    "type": "git",
    "url": "http://gitlab.orcadt.com/Orcadt/OrcaFS-UI.git"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "antd": "3.3.0",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-import": "1.3.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-app": "3.1.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "cluster": "~0.7.7",
    "cron": "~1.3.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "echarts": "~3.8.5",
    "eslint": "4.10.0",
    "eslint-config-react-app": "~2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "~1.1.6",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jquery": "~3.3.1",
    "js-cookie": "~2.2.0",
    "js-md5": "~0.7.3",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.3.0",
    "koa-socket": "^4.4.0",
    "less": "~2.7.3",
    "less-loader": "~4.0.5",
    "less-plugin-autoprefix": "~1.5.1",
    "log4js": "^2.5.2",
    "mongoose": "^5.0.1",
    "nodemailer": "^4.4.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "preload-webpack-plugin": "^2.3.0",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "rc-queue-anim": "^1.4.1",
    "react": "16.2.0",
    "react-addons-update": "15.6.2",
    "react-dev-utils": "4.2.1",
    "react-dom": "16.2.0",
    "react-hot-loader": "3.1.3",
    "react-redux": "5.0.5",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "require-dir": "0.3.2",
    "socket.io-client": "~2.0.4",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3",
    "xml2js": "^0.4.19"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": {
    "/socket.io/*": {
      "target": "http://localhost:8333",
      "ws": true
    },
    "/api/*": {
      "target": "http://localhost:8333",
      "changeOrigin": true,
      "secure": false
    }
  }
}
